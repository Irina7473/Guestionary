CREATE TABLE table_users
(
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT ,
    surname TEXT NOT NULL ,
    firstname TEXT NOT NULL ,
    phone TEXT NOT NULL ,
    mail TEXT NOT NULL
);

